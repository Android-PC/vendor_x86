---
 Android.bp | 18 ------------------
 1 file changed, 18 deletions(-)

diff --git a/Android.bp b/Android.bp
index 1b75c71..b94cd64 100644
--- a/Android.bp
+++ b/Android.bp
@@ -86,21 +86,3 @@ cc_binary {
     local_include_dirs: ["label"],
     shared_libs: ["libexfat"],
 }
-
-// Libraries for recovery
-
-cc_library_static {
-    name: "libexfat_mkfs",
-    defaults: ["exfat_defaults"],
-    srcs: ["mkfs/*.c"],
-    cflags: ["-Dmain=mkfs_exfat_main"],
-    static_libs: ["libexfat"],
-}
-
-cc_library_static {
-    name: "libexfat_fsck",
-    defaults: ["exfat_defaults"],
-    srcs: ["fsck/main.c"],
-    cflags: ["-Dmain=fsck_exfat_main"],
-    static_libs: ["libexfat"],
-}
